# cat << EOF | podman build --pull=always -f -
cat << EOF | podman build --pull=newer -f -

FROM fedora:39

RUN dnf install -y \
 binutils  \
 cargo  \
 curl  \
 gcc  \
 git  \
 make  \
 rust  \
 rust-aes-gcm+aes-devel  \
 rust-aes-gcm+alloc-devel  \
 rust-bitflags-devel  \
 rust-log-devel  \
 rust-memoffset-devel  \
 rust-proc-macro2-devel  \
 rust-quote-devel  \
 rust-std-static-x86_64-unknown-none  \
 rust-syn-devel  \
 rust-unicode-ident-devel  \
 rust-zerocopy-derive-devel  \
 rust-zerocopy-devel  \
 rust-zerocopy+byteorder-devel

RUN ls /usr/share/cargo/registry/aes-gcm-0.10.3/

RUN rpm -qa | grep rust-aes

EOF
